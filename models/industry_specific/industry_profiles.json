{
  "SaaS": {
    "key_metrics": [
      "net_dollar_retention_percent",
      "ltv_cac_ratio",
      "gross_margin_percent"
    ],
    "success_thresholds": {
      "net_dollar_retention_percent": 110,
      "ltv_cac_ratio": 3,
      "gross_margin_percent": 70,
      "monthly_recurring_revenue": 100000,
      "churn_rate": 5
    },
    "feature_weights": {
      "recurring_revenue": 0.35,
      "unit_economics": 0.3,
      "growth_efficiency": 0.2,
      "market_position": 0.15
    }
  },
  "FinTech": {
    "key_metrics": [
      "regulatory_advantage_present",
      "has_data_moat",
      "gross_margin_percent"
    ],
    "success_thresholds": {
      "regulatory_compliance": 1,
      "security_score": 0.8,
      "transaction_volume": 1000000,
      "fraud_rate": 0.1
    },
    "feature_weights": {
      "regulatory_position": 0.3,
      "trust_security": 0.25,
      "scale_efficiency": 0.25,
      "partnerships": 0.2
    }
  },
  "HealthTech": {
